{
  "name": "kvs-parser",
  "version": "0.0.2",
  "description": "AWS Kinesis Video Stream Parser Library",
  "main": "index.js",
  "scripts": {
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alezanai/kvs-parser.git"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "keywords": [
    "kinesis",
    "video",
    "stream",
    "kvs",
    "parser",
    "js",
    "stream",
    "aws",
    "encoding",
    "ebml",
    "decoding"
  ],
  "author": "Pierre Colle <piercus@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alezanai/kvs-parser/issues"
  },
  "homepage": "https://github.com/alezanai/kvs-parser#readme",
  "dependencies": {
    "aws-sdk": "^2.1090.0",
    "beamcoder": "^0.6.15",
    "ebml-stream": "^1.0.3"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "ava": "^4.0.1",
    "semantic-release": "^19.0.2",
    "winston": "^3.6.0",
    "xo": "^0.48.0"
  },
  "ava": {
    "files": [
      "!test/mock/*"
    ]
  },
  "xo": {
    "rules": {
      "ava/prefer-async-await": 0,
      "unicorn/prefer-module": 0,
      "camelcase": 0,
      "ava/no-skip-test": 0,
      "ava/no-ignored-test-files": 0
    }
  }
}
